{
    "name": "timesheet-future",
    "version": "1.0.0",
    "description": "Timesheet future",
    "scripts": {
        "build": "webpack --env.dll --env.aot",
        "build:aot": "npm run ngc -- -p tsconfig.browser.json",
        "build:min": "webpack --env.aot --mode=production",
        "build:debug": "npm run build -- --env.debug",
        "build:min:debug": "npm run build:min -- --env.debug",
        "prebuild": "npm run check:dev:build",
        "prebuild:aot": "npm run prebuild:min",
        "prebuild:min": "npm run clean",
        "prebuild:debug": "npm run prebuild",
        "prebuild:min:debug": "npm run prebuild:min",
        "postbuild": "npm run postbuild:min",
        "postbuild:aot": "npm run postbuild:min",
        "postbuild:min": "npm run clean:tmp",
        "postbuild:debug": "npm run postbuild:min",
        "postbuild:min:debug": "npm run postbuild:min",
        "build:prepare:dev": "webpack --config webpack.config.dev.js",
        "check:dev:build": "node ifMissingDevDeps.js && (npm run clean && npm run build:prepare:dev) || echo dependecies.js is present",
        "check:dev:start": "node ifMissingDevDeps.js && (npm run build) || echo dependecies.js is present",
        "clean": "rimraf wwwroot testResults && npm run clean:tmp",
        "clean:tmp": "rimraf build",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "ngc": "ngc",
        "ngcc": "ivy-ngcc",
        "postinstall": "npm run ngcc && npm run build:prepare:dev && npm run build",
        "server": "node server.js",
        "prestart": "npm run check:dev:start",
        "start": "npm run server -- --webpack"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "author": "Marek Bukovsk√Ω<marek.bukovsky@asseco-ce.com>",
    "license": "MIT",
    "dependencies": {
        "@angular/animations": "8.2.0",
        "@angular/common": "8.2.0",
        "@angular/compiler": "8.2.0",
        "@angular/core": "^8.2.0",
        "@angular/forms": "8.2.0",
        "@angular/platform-browser": "8.2.0",
        "@angular/platform-browser-dynamic": "8.2.0",
        "@angular/router": "8.2.0",
        "@angular/service-worker": "8.2.0",
        "core-js": "2.6.9",
        "rxjs": "6.5.2",
        "zone.js": "0.10.0"
    },
    "devDependencies": {
        "@types/node": "11.13.2",
        "@types/webpack": "4.4.32",
        "@types/connect": "3.4.32",
        "@types/jquery": "3.3.29",
        "@types/typeahead": "0.11.32",
        "@types/bootstrap": "~3.3.36",
        "@types/storejs": "2.0.3",
        "@types/konami.js": "1.4.28",
        "@angular/compiler-cli": "8.2.0",
        "@angular/language-service": "8.2.0",
        "@ngtools/webpack": "8.2.0",
        "node-sass": "4.10.0",
        "raw-loader": "1.0.0",
        "expose-loader": "0.7.5",
        "exports-loader": "0.7.0",
        "sass-loader": "7.1.0",
        "css-loader": "2.1.1",
        "file-loader": "3.0.1",
        "style-loader": "0.23.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "copy-webpack-plugin": "5.0.3",
        "html-webpack-include-assets-plugin": "1.0.7",
        "mini-css-extract-plugin": "0.8.0",
        "script-ext-html-webpack-plugin": "2.1.4",
        "webpack-notifier": "1.8.0",
        "compression-webpack-plugin": "3.0.0",
        "gulp": "3.9.1",
        "gulp-sass": "4.0.2",
        "gulp-watch": "5.0.1",
        "yargs": "13.3.0",
        "through2": "3.0.1",
        "concurrently": "4.1.1",
        "connect": "3.7.0",
        "connect-gzip-static": "2.1.1",
        "connect-history-api-fallback": "1.6.0",
        "http-proxy-middleware": "0.19.1",
        "webpack-dev-middleware": "3.7.0",
        "webpack-hot-middleware": "2.25.0",
        "speed-measure-webpack-plugin": "1.3.1",
        "webpack-bundle-analyzer": "3.3.2",
        "copyfiles": "2.1.1",
        "rimraf": "2.6.3",
        "webpack-cli": "3.3.6",
        "webpack": "4.39.0",
        "typescript": "3.5.3"
    }
}
